{
  "name": "frontend",
  "version": "0.1.0",
  "description": "mevn-scaffold front-end",
  "author": "Diego Chiquero Mena",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test": "env NODE_ENV=test cucumber-js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^2.6.11",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.3",
    "vue-sweetalert2": "^2.1.5",
    "vuetify": "^2.3.10",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-pwa": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.5.4",
    "@vue/cli-service": "^4.5.4",
    "@vue/eslint-config-prettier": "^5.1.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "chromedriver": "^84.0.1",
    "cucumber": "^6.0.5",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^5.0.0",
    "mongoose": "^5.12.6",
    "prettier": "^1.19.1",
    "sass": "^1.26.10",
    "sass-loader": "^7.1.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "vue-cli-plugin-vuetify": "^1.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "homepage": "https://github.com/diegochiquero/mevn-scaffold/blob/master/frontend/README.md",
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "vuetify",
    "vue-sweetalert2",
    "Jest",
    "Cucumber",
    "Selenium"
  ],
  "license": "MIT"
}
